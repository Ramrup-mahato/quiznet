{
    "name": "My Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [],
        "matches": ["<all_urls>"]
      }
    ],
    "permissions": [
      "tabs",
      "<all_urls>"
    ]
  }
  